{
  "manifest_version": 2,
  "name": "Unepochify Timestamp",
  "description": "Convert Unix timestamps to human-readable dates from the context menu",
  "version": "1.0.0",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "permissions": [
    "menus",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "unepochify-timestamp@example.com",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  }
}
